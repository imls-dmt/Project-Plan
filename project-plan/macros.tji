macro SPRINT [
        	task plan "Plan" {
        		allocate kb,nh,web,ab
        		effort 8h
        		priority 600
        		limits {maximum 2h {resources kb,nh,web,ab}} 
        	}
        	task execute "Execution"{
        		allocate web
        		effort ${1}
        		priority 600
        		depends !plan
        	}
        	task review "Review"{
				allocate kb,nh,web,ab
        		effort 8h
        		priority 600
        		limits {maximum 2h {resources kb,nh,web,ab}}
        	}
]

